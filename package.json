{
  "name": "wayke-ecom-react-bundler",
  "version": "3.3.0",
  "description": "A bundler for Waykes E-commerce React Component",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js --progress --profile --bail --mode production",
    "build:ie11": "webpack --config webpack.config.ie11.js --progress --profile --bail --mode production",
    "prebuild": "npm run clean",
    "prebuild:ie11": "npm run clean",
    "clean": "rimraf ./build",
    "lint": "eslint -c .eslintrc.js --ext .ts,.tsx 'src/**/*.{ts,tsx}'",
    "prettify": "prettier --write 'src/**/*.{ts,tsx}'",
    "prepare": "npm run prettify && npm run type-check && npm run lint",
    "type-check": "tsc --noEmit --skipLibCheck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wayke/wayke-ecom-bundler.git"
  },
  "keywords": [
    "wayke",
    "ecom"
  ],
  "author": "Björn Vikström",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wayke/wayke-ecom-bundler/issues"
  },
  "homepage": "https://github.com/wayke/wayke-ecom-bundler#readme",
  "engines": {
    "node": ">= 10.0.0"
  },
  "dependencies": {
    "@wayke-se/ecom-react": "^3.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/react": "^16.9.54",
    "@types/react-dom": "^16.9.9",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "babel-loader": "^8.2.2",
    "core-js": "^3.8.1",
    "css-loader": "^5.0.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "^6.2.0",
    "prettier": "^2.1.2",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.5",
    "webpack": "^5.3.0",
    "webpack-cli": "^4.1.0",
    "whatwg-fetch": "^3.5.0"
  }
}
